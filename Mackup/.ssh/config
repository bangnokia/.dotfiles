# Fig SSH Integration: Enabled
Include ~/.fig/ssh
# End of Fig SSH Integration

#default account
Host github.com
   HostName github.com
   User git
   IdentityFile ~/.ssh/id_rsa

#bangntq account
Host github.com-e-falcon-co-ltd
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa_bangntq

Host github.com-bangntq
	HostName github.com
	User git
	IdentityFile ~/.ssh/id_rsa_bangntq
